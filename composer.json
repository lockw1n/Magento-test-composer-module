{
  "name": "lockw1n/Magento-test-composer-module",
  "type": "magento-module",
  "license":"OSL-3.0",
  "description":"Education module, configured as composer module for Magento EE",
  "authors":[
    {
      "name":"Aleksey Kotsuba",
      "email":"aleksey.kotsuba@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:lockw1n/Magento-test-composer-module.git"
    }
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "map": [
      ["app/code/local/Symmetrics/Education", "app/code/local/Symmetrics/Education"],
      ["app/design/frontend/base/default/layout/education.xml", "app/design/frontend/base/default/layout/education.xml"],
      ["app/design/frontend/base/default/template/symmetrics/education", "app/design/frontend/base/default/template/symmetrics/education"],
      ["app/etc/modules/Symmetrics_Education.xml", "app/etc/modules/Symmetrics_Education.xml"]
    ]
  }
}